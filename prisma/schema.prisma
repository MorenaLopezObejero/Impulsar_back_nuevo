generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Typo_emprendimiento {
  typo String @id
  descripcion String
  foto String?

  producto Productos[]
}

model Productos {
  nombre String @id
  descripcion String
  pasos String
  herramientas String
  tipo String
  foto String?

  rela_prod_typo Typo_emprendimiento @relation(fields: [tipo], references: [typo])

}